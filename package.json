{
  "name": "angular-webpack-seed",
  "version": "0.0.0",
  "description": "Project seed for angular.js using Webpack as a module bundler",
  "scripts": {
    "webdriver-manager": "webdriver-manager",
    "webdriver": "npm run webdriver-manager update && npm run webdriver-manager start",
    "dev-server": "webpack-dev-server --config tools/webpack.config.js",
    "build": "webpack --config tools/webpack.config.js",
    "unit": "karma start tools/karma.conf.js",
    "e2e": "protractor tools/protractor.conf.js",
    "test": "karma start tools/karma.conf.js --single-run && npm run e2e",
    "start": "npm run dev-server"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/alexpods/angular-webpack-seed"
  },
  "keywords": [
    "angular",
    "webpack",
    "seed"
  ],
  "author": "alexpods <aleksey.podskrebyshev@gmail.com>",
  "homepage": "https://github.com/alexpods/angular-webpack-seed",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alexpods/angular-webpack-seed/issues"
  },
  "dependencies": {
    "angular": "^1.4.7"
  },
  "devDependencies": {
    "babel-core": "^6.1.2",
    "babel-loader": "^6.0.1",
    "babel-preset-es2015": "^6.1.2",
    "jasmine-core": "^2.3.4",
    "karma": "^0.13.15",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-jasmine": "^0.3.6",
    "karma-phantomjs-launcher": "^0.2.1",
    "karma-webpack": "^1.7.0",
    "phantomjs": "^1.9.18",
    "protractor": "^2.5.1",
    "webpack": "^1.12.3",
    "webpack-dev-server": "^1.12.1"
  }
}
